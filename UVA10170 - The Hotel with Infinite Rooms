#include <stdio.h>
#include <stdlib.h>

int main()
{
    long long S,D;

    while(scanf("%lld%lld",&S,&D)!=EOF){
        long long sum=0;
        for(sum;sum<D;S++){
            sum+=S;
            if(sum >= D){
                printf("%lld\n",S);
                break;
            }
        }
    }

    return 0;
}
