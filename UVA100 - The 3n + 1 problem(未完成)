#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n1,n2,n,counter = 0;
    scanf("%d",n1,n2);
    n=n2;

    while(n < n1){
        printf("%d ",n);
        ++counter;
        if(n<n1){
            //++counter;
            //printf("%d ",n);
            break;
        }
        else{
            if(n%2!=0)
                n=3*n+1;2
            else
                n=n/2;
        }
    }
    return 0;
}
