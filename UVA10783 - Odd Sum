#include <stdio.h>
#include <stdlib.h>

int main()
{
    int n;
    scanf("%d",&n);

    for(int i=1;i<=n;i++){//present the case number
        int a[2]={0},sum=0;
        scanf("%d%d",&a[0],&a[1]);//the value of each case

        for(int k=a[0];k<=a[1];k++){

            if(k%2!=0)
                sum+=k;
        }
        printf("Case %d: %d\n",i,sum);

    }
    return 0;
}
